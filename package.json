{
  "name": "aha-develop.azure-repos",
  "description": "Azure Repos Integration",
  "version": "0.0.0",
  "author": "Aha! (support@aha.io)",
  "repository": {
    "type": "git",
    "url": "https://github.com/aha-develop/azure-devops-repos"
  },
  "license": "MIT",
  "scripts": {
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@aha-app/aha-develop-react": "^1.0.6",
    "axios": "^0.25.0",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "prettier": "^2.5.1"
  },
  "ahaExtension": {
    "cspSources": [
      "https://dev.azure.com"
    ],
    "contributes": {
      "views": {
        "links": {
          "title": "Azure Repos",
          "entryPoint": "src/views/attribute.tsx",
          "host": "attribute",
          "recordTypes": [
            "Feature",
            "Epic",
            "Requirement"
          ]
        }
      },
      "commands": {
        "sync": {
          "title": "Azure Repos: Sync pull requests",
          "entryPoint": "src/commands/sync.ts"
        },
        "addLink": {
          "title": "Azure Repos: Link pull request to record",
          "entryPoint": "src/commands/addLink.ts"
        },
        "removeLinks": {
          "title": "Azure Repos: Remove linked pull requests from record",
          "entryPoint": "src/commands/removeLinks.ts"
        }
      },
      "endpoints": {
        "webhook": {
          "title": "Webhook from Azure Repos",
          "entryPoint": "src/webhooks/webhook.ts",
          "public": true
        }
      }
    }
  }
}
